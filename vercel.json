{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "config": {
                "includeFiles": [
                    "data.json"
                ]
            },
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}